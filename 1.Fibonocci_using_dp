//Fibonocii using Memoization
#include<bits/stdc++.h>
using namespace std;
int Fun(int n,vector<int> &dp){
    if(n<=1){
        return n;
    }
    if(dp[n]!=-1) return dp[n];
    dp[n]=Fun(n-1,dp)+Fun(n-2,dp);
    return dp[n];
}

int main(){
    int n;
    cin>>n;
    vector<int> dp(n+1,-1);
    cout<<Fun(n,dp);
}

